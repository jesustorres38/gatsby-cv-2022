{
  "name": "gatsby-starter-hello-world",
  "homepage": "https://jesustorres38.github.io/",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "deploy": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby build && gh-pages -d public -b gh-pages",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby build",
    "develop": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby develop",
    "local": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby develop",
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby serve",
    "clean": "export NODE_OPTIONS=--openssl-legacy-provider; gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^3.13.1",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "prettier": "2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
